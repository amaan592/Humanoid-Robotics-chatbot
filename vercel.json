{
  "version": 2,
  "builds": [
    {
      "src": "textbook/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/textbook/build/index.html"
    }
  ],
  "github": {
    "enabled": true
  }
}