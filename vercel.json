{
  "version": 2,
  "builds": [
    {
      "src": "textbook/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/^(?!.*\\.(?:js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot|json|xml|txt|map)$).*$/",
      "destination": "/index.html"
    }
  ],
  "github": {
    "enabled": true
  }
}
